{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "paths": {
      "@/common/*": [
        "./src/common/*"
      ],
      "@/onebot11/*": [
        "./src/onebot11"
      ],
      "@/ntqqapi/*": [
        "./src/ntqqapi/*"
      ]
    },
    "noEmit": true
  },
  "include": [
    "src/*",
    "src/**/*",
    "scripts/*"
  ],
  "exclude": [
    "node_modules"
  ]
}