{
  "compilerOptions": {
    "target": "ES2023",
    "module": "CommonJS",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "paths": {
      "@/common/*": [
        "./src/common/*"
      ],
      "@/onebot11/*": [
        "./src/onebot11/*"
      ],
      "@/ntqqapi/*": [
        "./src/ntqqapi/*"
      ]
    },
    "noEmit": true,
    "types": [
      "electron-vite/node"
    ]
  },
  "include": [
    "src",
    "scripts"
  ]
}
